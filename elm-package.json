{
    "version": "1.0.0",
    "summary": "Elm library for computer chess",
    "repository": "https://github.com/romstad/elm-chess.git",
    "license": "BSD3",
    "source-directories": [
        "src"
    ],
    "exposed-modules": [
        "Game",
        "Move",
        "Notation",
        "Piece",
        "PieceColor",
        "PieceType",
        "Position",
        "Square",
        "SquareFile",
        "SquareRank"
    ],
    "dependencies": {
        "elm-lang/core": "5.1.1 <= v < 6.0.0",
        "elm-tools/parser": "2.0.1 <= v < 3.0.0"
    },
    "elm-version": "0.18.0 <= v < 0.19.0"
}
